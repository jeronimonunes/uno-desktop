<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>

<AnchorPane fx:id="page" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufmg.dcc.pm.uno.controller.StartController">
	<stylesheets>
		<URL value="@Start.css"/>
	</stylesheets>
	<VBox spacing="10" AnchorPane.topAnchor="50.0" AnchorPane.rightAnchor="30.0">
		<Button fx:id="startBtn" text="Iniciar Jogo" maxWidth="1000" onAction="#start"/>
		<Button fx:id="exitBtn" text="Sair" maxWidth="1000" onAction="#exit"/>
	</VBox>
</AnchorPane>

